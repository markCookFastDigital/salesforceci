  <builders>
    <hudson.tasks.Shell>
      <command>echo ${JENKINS_HOME}/workspace/${ITEM_FULLNAME}src</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Ant plugin="ant@1.11">
      <targets>-lib
/var/jenkins_home/CI/ant-salesforce.jar
-propertyfile
/var/jenkins_home/CI/build.properties deployUnpackaged</targets>
      <antName>(Default)</antName>
      <buildFile>/var/jenkins_home/CI/build.xml</buildFile>
      <properties>sf.username  = $SalesforceUsername
sf.password = $SalesforcePassword
sf.serverurl = $SalesforceUrl
sf.deployFolder = ${WORKSPACE}/src
sf.checkOnly = $CheckBuild
sf.testLevel = $TestLevel
sf.logLevel = None</properties>
    </hudson.tasks.Ant>
    <hudson.tasks.Shell>
      <command>mkdir src2</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Ant plugin="ant@1.11">
      <targets>-lib
/var/jenkins_home/CI/ant-salesforce.jar
-propertyfile
/var/jenkins_home/CI/build.properties retrieveUnpackaged</targets>
      <antName>Default</antName>
      <buildFile>/var/jenkins_home/CI/build.xml</buildFile>
      <properties>sf.username  = $SalesforceUsername
sf.password = $SalesforcePassword
sf.serverurl = $SalesforceUrl
sf.retrieveUnpackaged = ${WORKSPACE}/src2
sf.package =${WORKSPACE}/src/package.xml
sf.checkOnly = $CheckBuild
sf.testLevel = $TestLevel
sf.logLevel = None</properties>
    </hudson.tasks.Ant>
    <hudson.tasks.Shell>
      <command>git checkout origin/$target &amp;&amp; cp -rf src2/* src/ &amp;&amp; git add src &amp;&amp; git commit -m &apos;Post pulldown commit for release - ${feature}&apos; &amp;&amp; git push -u origin HEAD</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers>
    <hudson.plugins.timestamper.TimestamperBuildWrapper plugin="timestamper@1.11.2"/>
  </buildWrappers>
