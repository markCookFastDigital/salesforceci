<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>Builds into Salesforce destructively</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <jenkins.model.BuildDiscarderProperty>
      <strategy class="hudson.tasks.LogRotator">
        <daysToKeep>14</daysToKeep>
        <numToKeep>20</numToKeep>
        <artifactDaysToKeep>-1</artifactDaysToKeep>
        <artifactNumToKeep>-1</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <com.sonyericsson.rebuild.RebuildSettings plugin="rebuild@1.27">
      <autoRebuild>false</autoRebuild>
      <rebuildDisabled>false</rebuildDisabled>
    </com.sonyericsson.rebuild.RebuildSettings>
    <hudson.plugins.svn__partial__release__mgr.ui.JobPropertyImpl plugin="svn-partial-release-mgr@1.0.1"/>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.ChoiceParameterDefinition>
          <name>SFUSERNAME</name>
          <description>Salesforce username</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>dave.liskevich@mm.coop</string>
              <string>dave.liskevich@mm.coop.mmdev</string>
              <string>dave.liskevich@mm.coop.st</string>
              <string>dave.liskevich@mm.coop.sit2</string>
              <string>dave.liskevich@mm.coop.preprod</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>SFPASSWORD</name>
          <description>Salesforce password</description>
          <defaultValue></defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>IsSandbox</name>
          <description></description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>test.salesforce.com</string>
              <string>login.salesforce.com</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>TestLevel</name>
          <description>What test level for the deployment</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>NoTestRun</string>
              <string>RunLocalTests</string>
              <string>RunAllTestsInOrg</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>CheckBuildOnly</name>
          <description>Determines whether the deployment is just validated or actually deployed</description>
          <defaultValue>true</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.scm.listtagsparameter.ListSubversionTagsParameterDefinition plugin="subversion@2.10.2">
          <name>SVNTAG</name>
          <description>Select a Subversion entry</description>
          <tagsDir>https://sfci.unitt.net/svn/cap-mm-coop/tags</tagsDir>
          <credentialsId>addbe267-c449-49ac-a4eb-4cbad1aec18f</credentialsId>
          <tagsFilter></tagsFilter>
          <reverseByDate>true</reverseByDate>
          <reverseByName>false</reverseByName>
          <defaultValue></defaultValue>
          <maxTags></maxTags>
        </hudson.scm.listtagsparameter.ListSubversionTagsParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.SubversionSCM" plugin="subversion@2.10.2">
    <locations>
      <hudson.scm.SubversionSCM_-ModuleLocation>
        <remote>https://sfci.unitt.net/svn/cap-mm-coop/tags/$SVNTAG/src</remote>
        <credentialsId>8cd9c5f9-65fe-4623-a453-aa95366372d1</credentialsId>
        <local>src/</local>
        <depthOption>infinity</depthOption>
        <ignoreExternalsOption>true</ignoreExternalsOption>
      </hudson.scm.SubversionSCM_-ModuleLocation>
      <hudson.scm.SubversionSCM_-ModuleLocation>
        <remote>https://sfci.unitt.net/svn/cap-mm-coop/tags/$SVNTAG/package</remote>
        <credentialsId>8cd9c5f9-65fe-4623-a453-aa95366372d1</credentialsId>
        <local>package/</local>
        <depthOption>infinity</depthOption>
        <ignoreExternalsOption>true</ignoreExternalsOption>
      </hudson.scm.SubversionSCM_-ModuleLocation>
      <hudson.scm.SubversionSCM_-ModuleLocation>
        <remote>https://sfci.unitt.net/svn/cap-mm-coop/branches/CI</remote>
        <credentialsId>8cd9c5f9-65fe-4623-a453-aa95366372d1</credentialsId>
        <local>CI/</local>
        <depthOption>infinity</depthOption>
        <ignoreExternalsOption>true</ignoreExternalsOption>
      </hudson.scm.SubversionSCM_-ModuleLocation>
    </locations>
    <excludedRegions></excludedRegions>
    <includedRegions></includedRegions>
    <excludedUsers></excludedUsers>
    <excludedRevprop></excludedRevprop>
    <excludedCommitMessages></excludedCommitMessages>
    <workspaceUpdater class="hudson.scm.subversion.UpdateUpdater"/>
    <ignoreDirPropChanges>false</ignoreDirPropChanges>
    <filterChangelog>false</filterChangelog>
    <quietOperation>false</quietOperation>
  </scm>
  <quietPeriod>600</quietPeriod>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>cp ./CI/buildfiles/cleanupBuild/cleanBuildFile.class .</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>java cleanBuildFile no</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>mv /var/lib/jenkins/workspace/${JOB_NAME}/package/package.xml /var/lib/jenkins/workspace/${JOB_NAME}/src/package.xml </command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>mv /var/lib/jenkins/workspace/${JOB_NAME}/package/destructiveChanges.xml /var/lib/jenkins/workspace/${JOB_NAME}/src/destructiveChanges.xml </command>
    </hudson.tasks.Shell>
    <hudson.tasks.Ant plugin="ant@1.8">
      <targets>-lib
/var/lib/jenkins/workspace/${JOB_NAME}/CI/dataLoader_jars/ant-salesforce.jar
-propertyfile
/var/lib/jenkins/workspace/${JOB_NAME}/CI/buildfiles/build.properties</targets>
      <antName>(Default)</antName>
      <buildFile>CI/buildfiles/build.xml</buildFile>
      <properties>sf.username  = $SFUSERNAME
sf.password = $SFPASSWORD
sf.serverurl = $IsSandbox
sf.checkOnly = $CheckBuildOnly
sf.runAllTests = $TestLevel
sf.logLevel = None</properties>
    </hudson.tasks.Ant>
    <org.jenkinsci.plugins.buildnameupdater.BuildNameUpdater plugin="build-name-setter@1.6.8">
      <buildName>version.txt</buildName>
      <macroTemplate>${SVNTAG}-CheckBuild-$CheckBuildOnly-TestLevel-${TestLevel}</macroTemplate>
      <fromFile>false</fromFile>
      <fromMacro>true</fromMacro>
      <macroFirst>false</macroFirst>
    </org.jenkinsci.plugins.buildnameupdater.BuildNameUpdater>
  </builders>
  <publishers>
    <hudson.tasks.Mailer plugin="mailer@1.20">
      <recipients></recipients>
      <dontNotifyEveryUnstableBuild>false</dontNotifyEveryUnstableBuild>
      <sendToIndividuals>false</sendToIndividuals>
    </hudson.tasks.Mailer>
  </publishers>
  <buildWrappers/>
</project>