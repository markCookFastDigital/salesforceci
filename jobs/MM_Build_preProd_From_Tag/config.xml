<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>Builds a tag into a salesforce org/environment</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <jenkins.model.BuildDiscarderProperty>
      <strategy class="hudson.tasks.LogRotator">
        <daysToKeep>14</daysToKeep>
        <numToKeep>20</numToKeep>
        <artifactDaysToKeep>-1</artifactDaysToKeep>
        <artifactNumToKeep>-1</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <com.sonyericsson.rebuild.RebuildSettings plugin="rebuild@1.25">
      <autoRebuild>false</autoRebuild>
      <rebuildDisabled>false</rebuildDisabled>
    </com.sonyericsson.rebuild.RebuildSettings>
    <hudson.plugins.svn__partial__release__mgr.ui.JobPropertyImpl plugin="svn-partial-release-mgr@1.0.1"/>
    <hudson.plugins.svn__release__mgr.JobPropertyImpl plugin="svn-release-mgr@1.2">
      <maxRevisions></maxRevisions>
    </hudson.plugins.svn__release__mgr.JobPropertyImpl>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.ChoiceParameterDefinition>
          <name>SalesforceUsername</name>
          <description>The Username for the Destination Salesforce Environment</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>dave.liskevich@mm.coop.preprod</string>
              <string>chris.unitt@mm.coop.preprod</string>
              <string>goverdhan.unitt@mm.coop.preprod</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.PasswordParameterDefinition>
          <name>SalesforcePassword</name>
          <description>The password AND Security Token for the destination Environment</description>
          <defaultValue>{}</defaultValue>
        </hudson.model.PasswordParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>CheckBuildOnly</name>
          <description>Determines if the build is run as a CheckOnly</description>
          <defaultValue>true</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>RepoURL</name>
          <description></description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>https://sfci.unitt.net/svn/cap-mm-coop</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.scm.listtagsparameter.ListSubversionTagsParameterDefinition plugin="subversion@2.9">
          <name>SVNTag</name>
          <description>Select a Subversion entry</description>
          <tagsDir>https://sfci.unitt.net/svn/cap-mm-coop/tags</tagsDir>
          <credentialsId>8cd9c5f9-65fe-4623-a453-aa95366372d1</credentialsId>
          <tagsFilter></tagsFilter>
          <reverseByDate>true</reverseByDate>
          <reverseByName>false</reverseByName>
          <defaultValue>NONE</defaultValue>
          <maxTags>20</maxTags>
        </hudson.scm.listtagsparameter.ListSubversionTagsParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>isSandbox</name>
          <description>Is this org a sandbox or a production environment?</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>test.salesforce.com</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>TestLevel</name>
          <description>What test level are we using for the deployment</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>NoTestRun</string>
              <string>RunLocalTests</string>
              <string>RunAllTestsInOrg</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>AutoGeneratePackageXML</name>
          <description>Determines whether a package.xml is autogenerated</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>yes</string>
              <string>no</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.SubversionSCM" plugin="subversion@2.9">
    <locations>
      <hudson.scm.SubversionSCM_-ModuleLocation>
        <remote>${RepoURL}/tags/$SVNTAG/src</remote>
        <credentialsId>8cd9c5f9-65fe-4623-a453-aa95366372d1</credentialsId>
        <local>src/</local>
        <depthOption>infinity</depthOption>
        <ignoreExternalsOption>true</ignoreExternalsOption>
      </hudson.scm.SubversionSCM_-ModuleLocation>
      <hudson.scm.SubversionSCM_-ModuleLocation>
        <remote>${RepoURL}/tags/$SVNTAG/package</remote>
        <credentialsId>8cd9c5f9-65fe-4623-a453-aa95366372d1</credentialsId>
        <local>package/</local>
        <depthOption>infinity</depthOption>
        <ignoreExternalsOption>true</ignoreExternalsOption>
      </hudson.scm.SubversionSCM_-ModuleLocation>
      <hudson.scm.SubversionSCM_-ModuleLocation>
        <remote>${RepoURL}/branches/CI</remote>
        <credentialsId>8cd9c5f9-65fe-4623-a453-aa95366372d1</credentialsId>
        <local>CI/</local>
        <depthOption>infinity</depthOption>
        <ignoreExternalsOption>true</ignoreExternalsOption>
      </hudson.scm.SubversionSCM_-ModuleLocation>
    </locations>
    <excludedRegions></excludedRegions>
    <includedRegions></includedRegions>
    <excludedUsers></excludedUsers>
    <excludedRevprop></excludedRevprop>
    <excludedCommitMessages></excludedCommitMessages>
    <workspaceUpdater class="hudson.scm.subversion.UpdateUpdater"/>
    <ignoreDirPropChanges>false</ignoreDirPropChanges>
    <filterChangelog>false</filterChangelog>
  </scm>
  <quietPeriod>600</quietPeriod>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>cp CI/buildfiles/cleanupBuild/* .</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>java cleanBuildFile $AutoGeneratePackageXML</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>mv ${JENKINS_HOME}/workspace/${JOB_NAME}/package/package.xml ${JENKINS_HOME}/workspace/${JOB_NAME}/src/package.xml </command>
    </hudson.tasks.Shell>
    <hudson.tasks.Ant plugin="ant@1.7">
      <targets>-lib
${JENKINS_HOME}/workspace/${JOB_NAME}/CI/dataLoader_jars/ant-salesforce.jar
-propertyfile
${JENKINS_HOME}/workspace/${JOB_NAME}/CI/buildfiles/build.properties</targets>
      <antName>(Default)</antName>
      <buildFile>CI/buildfiles/build.xml</buildFile>
      <properties>sf.username  = $SalesforceUsername
sf.password = $SalesforcePassword
sf.serverurl = $isSandbox
sf.checkOnly = $CheckBuildOnly
sf.runAllTests = true
sf.testLevel = $TestLevel
sf.logLevel = None</properties>
    </hudson.tasks.Ant>
  </builders>
  <publishers>
    <hudson.tasks.Mailer plugin="mailer@1.20">
      <recipients>dave.liskevich@capgemini.com chris.unitt@capgemini.com</recipients>
      <dontNotifyEveryUnstableBuild>false</dontNotifyEveryUnstableBuild>
      <sendToIndividuals>false</sendToIndividuals>
    </hudson.tasks.Mailer>
  </publishers>
  <buildWrappers>
    <org.jenkinsci.plugins.buildnamesetter.BuildNameSetter plugin="build-name-setter@1.6.7">
      <template>Preprod-${SVNTAG}-CheckBuild-$CheckBuildOnly-TestLevel-${TestLevel}-Revision-${ENV,var=&quot;SVN_REVISION&quot;}</template>
      <runAtStart>true</runAtStart>
      <runAtEnd>true</runAtEnd>
    </org.jenkinsci.plugins.buildnamesetter.BuildNameSetter>
  </buildWrappers>
</project>